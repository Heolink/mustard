{
    "name": "hamjoint/mustard-core",
    "description": "Vital functionality of Mustard, the auction and marketplace platform",
    "keywords": ["auctions", "marketplace", "ecommerce"],
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Tom Pitcher",
            "email": "tom@hamjoint.org"
        }
    ],
    "support": {
        "issues": "https://github.com/hamjoint/mustard-core/issues",
        "source": "https://github.com/hamjoint/mustard-core"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "modernizr/modernizr",
                "version": "2.8.3",
                "dist": {
                    "url": "https://github.com/Modernizr/Modernizr/archive/v2.8.3.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "najlepsiwebdesigner/foundation-datepicker",
                "version": "1.3.0",
                "dist": {
                    "url": "https://github.com/najlepsiwebdesigner/foundation-datepicker/archive/1.3.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "version": "1.4.1",
                "dist": {
                    "url": "https://github.com/kenwheeler/slick/archive/1.4.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "enyo/dropzone",
                "version": "4.0.1",
                "dist": {
                    "url": "https://github.com/enyo/dropzone/archive/v4.0.1.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "laravel/framework": "5.0.*",
        "jasonlewis/expressive-date": "~1.0.2",
        "doctrine/dbal": "~2.5.1",
        "erusev/parsedown": "~1.5.1",
        "pda/pheanstalk": "~2.1.1",
        "intervention/image": "~2.1.2",
        "stripe/stripe-php": "~2.1.1",
        "kriswallsmith/assetic": "~1.2.1",
        "leafo/scssphp": "~0.1.1",
        "natxet/cssmin": "~3.0.2",
        "mrclay/minify": "~3.0.0",
        "zurb/foundation": "~5.4.7",
        "frameworks/jquery": "~2.1.1",
        "modernizr/modernizr": "~2.8.3",
        "fortawesome/font-awesome": "~4.3.0",
        "najlepsiwebdesigner/foundation-datepicker": "~1.3.0",
        "kenwheeler/slick": "~1.4.1",
        "enyo/dropzone": "~4.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "psr-4": {
            "Mustard\\": "src/Mustard/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
